<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item
    android:left="-5dp"
    android:right="-5dp"
    android:top="-5dp">
    <shape
        android:shape="rectangle"
        >

        <stroke
            android:width="1dp"
            android:dashGap="2dp"
            android:dashWidth="2dp"
            android:color="@color/rwacolor" />
        <solid android:color="@android:color/transparent"/> 
    </shape>
</item>

</layer-list>